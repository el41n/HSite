    <script type="text/javascript">
        var getShopLink = function () {};
        var getGameplayLink = function () {};
        var getDownloadLink = function () {
            $.getJSON("https://downloadcenter.intel.com/ru//json/pageresults?pageNumber=1&productId=" + 126699 + "&keyword=&filterOS=&filterType=").done(function (dcPayload) {
                if (dcPayload && dcPayload.ResultsForDisplay && dcPayload.ResultsForDisplay.length > 0) {
                    $("#support-button-dc").attr("href", "https://downloadcenter.intel.com/ru/product/126699/Intel-Core-i9-7980XE-Extreme-Edition-Processor-24-75M-Cache-up-to-4-20-GHz-")
                    addResults(dcPayload.ResultsForDisplay, $("#drivers-list tbody"));
                    $("#tab-blade-1-7, #tab-side-1-7, #tab-label-1-7").removeAttr("style");
                }
            });
        };

        function addResults(results, $resultTable) {
            var downloadUrl = "https://downloadcenter.intel.com/ru/" + "download/";
            var $rowTemplate = $('<tr style="display: none;">' +
                '<td class="description">' +
                    '<a class="template-link-download"></a>' +
                    '<p class="template-description"></p>' +
                '</td>' +
                '<td class="collapsible-col collapsible3 type template-type"></td>' +
                '<td class="collapsible-col collapsible2 os template-os"></td>' +
                '<td class="collapsible-col collapsible1 version"><span class="template-version"></span><br /><span class="type template-version-status"></span></td>' +
                '<td class="collapsible-col collapsible4 date template-date"></td>' +
            '</tr>');

            $.map(results, function (result) {
                $thisRow = $rowTemplate.clone();
                $thisRow.find("a.template-link-download").attr("href", downloadUrl + result.Id + "/" + /* result.Title.urlifyName() + */ "?product=" + 126699);
                $thisRow.find("a.template-link-download").text(result.Title);
                $thisRow.find(".template-description").html(result.SummaryDescription);
                $thisRow.find(".template-os").html(result.OperatingSystemSet.join("<br />"));
                $thisRow.find(".template-date").text(result.PublishDate);
                $thisRow.find(".template-version").text(result.Version);
                $thisRow.find(".template-version-status").text(result.VersionStatus);
                $thisRow.find(".template-type").text(result.DownloadType);
                $thisRow.data("title", result.Title);
                $thisRow.data("version", result.Version);
                $thisRow.data("id", result.Id);
                $thisRow.appendTo($resultTable);
                $thisRow.fadeIn('slow');
            });
        }


        var setPanel = function (useFilter) {
            useFilter = useFilter.replace(/\W/g, "").toLowerCase();

            var $foundTarget = $('.tab-pane[data-hash="' + useFilter + '"]');
            if ($foundTarget.length === 1) {
                var tabId = $foundTarget.attr("id");
                // tab bar
                $('ul[role="tablist"] li').removeClass('active');
                $('a[href="#' + tabId + '"]').parents('li').addClass('active');
                // Headlines
                var $headlinebar = $foundTarget.children(".intel-element-toggler").first();
                $headlinebar.toggleClass('intel-element-toggler-active');
                $headlinebar.toggleClass('intel-element-toggler-inactive');
                $headlinebar.next().slideToggle();
                //show content
                $('.tab-pane').hide();
                $foundTarget.show().addClass('active');
            }
        }

        var breadcrumbHeight = $('.breadcrumb').height();
        var headerHeight = $("#recode50header").height() || 0;

        function checkNavSize() {
            $("ul.specs-nav").css({ "max-width" : $("#nav-shim").width() });

            var breadcrumbHeight = $('.breadcrumb').height();
            var headerHeight = $("#recode50header").height() || 0;
            var maxNavHeight = $(window).height() - headerHeight - breadcrumbHeight;

            if ($('.specs-nav').outerHeight(true) + 7 > maxNavHeight) {
                $('.specs-nav').addClass('max-nav').css('max-height', maxNavHeight - 10);
            } else {
                $('.specs-nav').removeClass('max-nav').css('max-height', 'none');
            }
            $('.specs-nav').affix('checkPosition');
        }

        $(document).ready(function () {
            getShopLink();
            getGameplayLink();
            getDownloadLink();
            if (document.location.hash) { setPanel(document.location.hash); }

            // Navigation setup
            $('body').scrollspy({
                target: '.specs-nav-container',
                offset: 180
            });
            $('.specs-nav').affix({
                offset: {
                    top: function () { return (this.top = $('.specs-nav').offset().top - ($('.breadcrumb').height() + ($("#recode50header").height() || 0))); },
                    bottom: function () { return (this.bottom = $(document).height() - ($('.footer-support-options').offset().top - 235)); }
                }
            });

            setTimeout(function() { checkNavSize(); }, 25); //delay(function () { checkNavSize(); }, 25);

        });

        if ($('.specs-nav').length > 0) {
            $(window).resize(function () {
                if (this.resizeTO) clearTimeout(this.resizeTO);
                this.resizeTO = setTimeout(function () { checkNavSize(); }, 500);
            });

            $('.spec-section-title').click(function (e) {
                e.preventDefault();
                var sectionAnchor = $(this).attr('href');
                var thisNavSection = $(this).parent('.specs-nav-section');

                $('.specs-section, .specs-nav-section').removeClass('active');
                $('.specs-nav-section').removeClass('always-active');
                $(sectionAnchor).addClass('active');
                $(thisNavSection).addClass('always-active');
                $('html,body').animate({ scrollTop: 0 }, 200);
            });

            $('.specs-nav-section ul li a').click(function (e) {
                e.preventDefault();
                var offsetPosition = $(".product-family-title").height() + $("#recode50header").height();
                var sectionAnchor = $(this).attr('href');
                var sectionAnchorPosition = $(sectionAnchor).position().top;

                $('html,body').animate({ scrollTop: sectionAnchorPosition + offsetPosition }, 500);
            });

            //accordion
            $('.specs-accordion-title').click(function () {
                if ($(this).hasClass('active')) {
                    $(this).removeClass('active').next().hide();
                } else {
                    $('.specs-accordion-title').removeClass('active').next().hide();
                    $('.specs-accordion-title').next().hide();
                    $(this).addClass('active').next().show();
                }
            });

        }
    </script>


        <script type="text/javascript">
            getShopLink = function () {
                var locales = {"RU": 1049, "MD": 2073 };
                var myLocale = locales[cookie("src_countrycode")]; 
                if (myLocale != undefined) {
                    $.get("https://shop-media.intel.com/api/v2/ProductService/FlagProductBySpec?locale=" + myLocale + "&q=i9-7980XE&apikey=YNn44iWqoECJnS5EdAwPoA==", function (d) { 
                        $data = $(d);
                        var hits = $data.find("Count").text();
                        if (hits > 0) {
                            $("a.btn-shop-for-product").attr("href", $data.find("Url").text() + "&cid=IntelARK");
                            $(".btn-shop-for-product").show();
                        }
                    },
                    "xml");
                }
            };
        </script>
    <text></text>





